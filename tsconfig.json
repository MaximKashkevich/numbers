{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": ["**/*.ts", "**/*.vue", "nuxt.config.js"],
  "exclude": ["node_modules"]
}
